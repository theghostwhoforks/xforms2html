<root xmlns:jr="http://openrosa.org/javarosa" xmlns:xf="http://www.w3.org/2002/xforms"
      xmlns:h="http://www.w3.org/1999/xhtml"
      xmlns:ev="http://www.w3.org/2001/xml-events"
      xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <model>
        <instance xmlns="http://www.w3.org/2002/xforms" xmlns:orx="http://openrosa.org/xforms/">
            <doctor_visit id="doctor_visit">
                <formhub>
                    <uuid/>
                </formhub>
                <today/>
                <date/>
                <doctor/>
                <beneficiary/>
                <visitReason/>
                <weight/>
                <height/>
                <temperature/>
                <bloodPressure/>
                <fundalHeight/>
                <chiefComplaint/>
                <assessment/>
                <healthProblems/>
                <healthProblemsOther/>
                <diagnosis/>
                <medications template="">
                    <medicationName/>
                    <dose/>
                    <unit/>
                    <duration/>
                    <instructions/>
                </medications>
                <tests template="">
                    <testRequired/>
                    <testRequiredName/>
                </tests>
                <hospitalVisit/>
                <followUp/>
                <clinicalNote/>
                <meta>
                    <instanceID/>
                </meta>
            </doctor_visit>
        </instance>
    </model>
</root>